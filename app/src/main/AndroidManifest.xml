<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.justjava.helpatease">

    <application
        android:name=".HelpAtEase"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ProfileActivity" />
        <activity
            android:name=".OrganisationActivity"
            android:label="@string/title_activity_organisation"
            android:theme="@style/AppTheme" />
        <activity android:name=".AddEventActivity" />
        <activity android:name=".EditEventActivity" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".VolunteerActivity"
            android:label="@string/title_activity_volunteer"
            android:theme="@style/AppTheme" />
        <activity android:name=".ViewmyEventActivity" />
        <activity android:name=".ViewVolunteerActivity"></activity>
    </application>

</manifest>